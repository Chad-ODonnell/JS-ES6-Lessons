// Define Global Variables.
let satellite = "The Moon";
let galaxy = "The Milky Way";
let stars = "North Star";

// Creates a Function Expression to Interpolate a string for return utilizing the Arrow Function methodology.
const callMyNightSky = () => (`Night Sky: ${satellite}, ${stars}, and ${galaxy}`);

// Using Object 'Console's' method 'log()' to invoke our Function Expression 'callMyNigthSky().'
console.log(callMyNightSky());
